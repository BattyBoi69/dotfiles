const fibonacci = function(n) {
	//Find the nth value in the fibonacci sequence: 1, 1, 2, 3, 5, 8...
	
	if (+n < 0) {
		return "OOPS";
	}

	let result = [0, 1];
	for (let i=+n; i>0; i--) {
		result = [result[1], result.reduce((a, b) => a + b, 0)];
	}
	return result[0];
};

// Do not edit below this line
module.exports = fibonacci;
